extends layout

block content
    nav.navbar.navbar-default.navbar-static-top
        .container-fluid
            .navbar-header
                button.collapsed.navbar-toggle(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-8', aria-expanded='false')
                    span.sr-only Toggle navigation
                    span.icon-bar
                    span.icon-bar
                    span.icon-bar
                a.navbar-brand(href='#') Vanguard
            #bs-example-navbar-collapse-8.collapse.navbar-collapse
                ul.nav.navbar-nav
                    li.active
                        a(href='#') Home
    div.col-sm-12
        h1.align Welcome Ryan!
        div.col-sm-12
            h1.align Vanguard Stocks/ETFs
            .row
                .col-sm-3(ng-repeat="fund in vanguardTickerData")
                    .thumbnail
                        img(src='images/vanguard.jpg', alt='...')
                        .caption
                            h3 Vanguard
                            p Ticker: {{fund.tickerName}}
                            p Price: {{fund.tickerPrice}}
                            p Amount holding:
                            p Total Amount:
            h1.align Other Stocks/ETFS
            .row
                .col-sm-3(ng-repeat="fund in nonVanguardTickerData")
                    .thumbnail
                        img(src='images/amazon.jpg', alt='...')
                        .caption
                            h3 Google
                            p Ticker: {{fund.tickerName}}
                            p Price: {{fund.tickerPrice}}
                            p Amount holding:
                            p Total Amount:
